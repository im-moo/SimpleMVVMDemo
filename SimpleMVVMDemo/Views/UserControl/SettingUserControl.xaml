<UserControl x:Class="SimpleMVVMDemo.Views.SettingUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:SimpleMVVMDemo.Helper"
             xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
             xmlns:viewmodel="clr-namespace:SimpleMVVMDemo.ViewModel"
             d:DataContext="{d:DesignInstance Type=viewmodel:SettingUserControlViewModel}"
             mc:Ignorable="d"
             d:DesignHeight="420"
             d:DesignWidth="300">
    <UserControl.DataContext>
        <viewmodel:SettingUserControlViewModel />
    </UserControl.DataContext>
    <Grid Background="WhiteSmoke">
        <Image Grid.Row="1"
               Source="/Image/wl.jpg"
               Opacity="0.3"
               HorizontalAlignment="Center"
               VerticalAlignment="Top"
               Margin="0,170,0,0"></Image>
    </Grid>
</UserControl>
